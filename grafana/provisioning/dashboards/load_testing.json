{
  "id": null,
  "uid": null,
  "title": "FastAPI - Load Testing Dashboard",
  "tags": ["fastapi", "load-testing", "locust", "performance"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 0,
  "refresh": "5s",
  "description": "Dashboard pour visualiser l'impact des tests de charge Locust sur les performances de l'API",
  "panels": [
    {
      "id": 1,
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "type": "stat",
      "title": "Total Requests",
      "targets": [
        {
          "expr": "api_requests_total",
          "refId": "A",
          "legendFormat": "Total"
        }
      ],
      "gridPos": {"x": 0, "y": 0, "w": 6, "h": 4},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "short"
        }
      }
    },
    {
      "id": 2,
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "type": "stat",
      "title": "Current Request Rate",
      "targets": [
        {
          "expr": "rate(api_requests_total[30s])",
          "refId": "A",
          "legendFormat": "req/s"
        }
      ],
      "gridPos": {"x": 6, "y": 0, "w": 6, "h": 4},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "unit": "reqps",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 10, "color": "yellow"},
              {"value": 50, "color": "orange"},
              {"value": 100, "color": "red"}
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "type": "stat",
      "title": "Mean Latency",
      "targets": [
        {
          "expr": "rate(api_request_latency_seconds_sum[30s]) / rate(api_request_latency_seconds_count[30s])",
          "refId": "A",
          "legendFormat": "Mean"
        }
      ],
      "gridPos": {"x": 12, "y": 0, "w": 6, "h": 4},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "unit": "s",
          "decimals": 3,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 0.1, "color": "yellow"},
              {"value": 0.5, "color": "orange"},
              {"value": 1.0, "color": "red"}
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "type": "stat",
      "title": "P95 Latency",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(api_request_latency_seconds_bucket[30s]))",
          "refId": "A",
          "legendFormat": "p95"
        }
      ],
      "gridPos": {"x": 18, "y": 0, "w": 6, "h": 4},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "unit": "s",
          "decimals": 3,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 0.1, "color": "yellow"},
              {"value": 0.5, "color": "orange"},
              {"value": 1.0, "color": "red"}
            ]
          }
        }
      }
    },
    {
      "id": 5,
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "type": "timeseries",
      "title": "Request Rate Over Time",
      "targets": [
        {
          "expr": "rate(api_requests_total[30s])",
          "refId": "A",
          "legendFormat": "Requests/sec"
        }
      ],
      "gridPos": {"x": 0, "y": 4, "w": 12, "h": 8},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "reqps",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "id": 6,
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "type": "timeseries",
      "title": "Latency Percentiles Under Load",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(api_request_latency_seconds_bucket[30s]))",
          "refId": "A",
          "legendFormat": "p50 (median)"
        },
        {
          "expr": "histogram_quantile(0.75, rate(api_request_latency_seconds_bucket[30s]))",
          "refId": "B",
          "legendFormat": "p75"
        },
        {
          "expr": "histogram_quantile(0.90, rate(api_request_latency_seconds_bucket[30s]))",
          "refId": "C",
          "legendFormat": "p90"
        },
        {
          "expr": "histogram_quantile(0.95, rate(api_request_latency_seconds_bucket[30s]))",
          "refId": "D",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, rate(api_request_latency_seconds_bucket[30s]))",
          "refId": "E",
          "legendFormat": "p99"
        }
      ],
      "gridPos": {"x": 12, "y": 4, "w": 12, "h": 8},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "s",
          "decimals": 3,
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          }
        }
      }
    },
    {
      "id": 7,
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "type": "timeseries",
      "title": "Mean Latency vs Request Rate",
      "targets": [
        {
          "expr": "rate(api_request_latency_seconds_sum[30s]) / rate(api_request_latency_seconds_count[30s])",
          "refId": "A",
          "legendFormat": "Mean Latency (s)"
        },
        {
          "expr": "rate(api_requests_total[30s])",
          "refId": "B",
          "legendFormat": "Request Rate (req/s)"
        }
      ],
      "gridPos": {"x": 0, "y": 12, "w": 12, "h": 8},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          }
        },
        "overrides": [
          {
            "matcher": {"id": "byName", "options": "Mean Latency (s)"},
            "properties": [
              {
                "id": "unit",
                "value": "s"
              },
              {
                "id": "custom.axisPlacement",
                "value": "left"
              }
            ]
          },
          {
            "matcher": {"id": "byName", "options": "Request Rate (req/s)"},
            "properties": [
              {
                "id": "unit",
                "value": "reqps"
              },
              {
                "id": "custom.axisPlacement",
                "value": "right"
              }
            ]
          }
        ]
      }
    },
    {
      "id": 8,
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "type": "heatmap",
      "title": "Latency Distribution Heatmap",
      "targets": [
        {
          "expr": "sum(rate(api_request_latency_seconds_bucket[30s])) by (le)",
          "refId": "A",
          "format": "heatmap",
          "legendFormat": "{{le}}"
        }
      ],
      "gridPos": {"x": 12, "y": 12, "w": 12, "h": 8},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "spectrum"},
          "unit": "short"
        }
      }
    },
    {
      "id": 9,
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "type": "timeseries",
      "title": "Request Rate - 1min vs 5min Average",
      "targets": [
        {
          "expr": "rate(api_requests_total[1m])",
          "refId": "A",
          "legendFormat": "1min avg"
        },
        {
          "expr": "rate(api_requests_total[5m])",
          "refId": "B",
          "legendFormat": "5min avg"
        }
      ],
      "gridPos": {"x": 0, "y": 20, "w": 12, "h": 8},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "reqps",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          }
        }
      }
    },
    {
      "id": 10,
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "type": "stat",
      "title": "P99 Latency",
      "targets": [
        {
          "expr": "histogram_quantile(0.99, rate(api_request_latency_seconds_bucket[30s]))",
          "refId": "A",
          "legendFormat": "p99"
        }
      ],
      "gridPos": {"x": 12, "y": 20, "w": 6, "h": 4},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "unit": "s",
          "decimals": 3,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 0.2, "color": "yellow"},
              {"value": 0.5, "color": "orange"},
              {"value": 1.0, "color": "red"}
            ]
          }
        }
      }
    },
    {
      "id": 11,
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "type": "stat",
      "title": "P50 Latency (Median)",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(api_request_latency_seconds_bucket[30s]))",
          "refId": "A",
          "legendFormat": "p50"
        }
      ],
      "gridPos": {"x": 18, "y": 20, "w": 6, "h": 4},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "unit": "s",
          "decimals": 3,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 0.05, "color": "yellow"},
              {"value": 0.1, "color": "orange"},
              {"value": 0.2, "color": "red"}
            ]
          }
        }
      }
    }
  ]
}

